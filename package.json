{
	"name": "tailwind-datepicker-react",
	"version": "1.0.1",
	"description": "A tailwindcss/flowbite datepicker component built as a react component with types",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"files": [
		"dist",
		"types"
	],
	"types": "types/index.d.ts",
	"scripts": {
		"build": "tsc --outDir dist --declarationDir types --declaration true --emitDeclarationOnly true && rollup -c"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/OMikkel/tailwind-datepicker-react.git"
	},
	"keywords": [
		"tailwind",
		"react",
		"flowbite",
		"nextjs",
		"datepicker",
		"dateselector",
		"dateinput",
		"date"
	],
	"author": "OMikkel",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/OMikkel/tailwind-datepicker-react/issues"
	},
	"homepage": "https://github.com/OMikkel/tailwind-datepicker-react#readme",
	"peerDependencies": {
		"react": "^18",
		"tailwindcss": "^3"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-node-resolve": "^14.1.0",
		"@types/react": "^18",
		"react": "^18",
		"rollup": "^2.79.1",
		"rollup-plugin-dts": "^4.2.2",
		"rollup-plugin-typescript2": "^0.34.1",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4"
	}
}
